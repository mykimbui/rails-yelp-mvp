<h1>Reviews#new</h1>
<p>Find me in app/views/reviews/new.html.erb</p>

<h1>Review for <%= @restaurant.name %></h1>


<% simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating %>
  <%= f.button :submit, "Send new review", class: "btn btn-danger" %>
<% end %>
